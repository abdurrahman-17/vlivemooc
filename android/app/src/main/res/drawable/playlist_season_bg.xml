<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:shape="rectangle" android:tint="#FFFFFF">
            <corners android:radius="@dimen/size_2dp" />
            <size android:width="79dp" android:height="33dp" />
        </shape>
    </item>

    <item android:state_checked="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/size_2dp" />
            <stroke android:width="@dimen/size_1dp" android:color="#FFFFFF" android:dashWidth="@dimen/size_8dp" />
            <size android:width="79dp" android:height="33dp" />
        </shape>
    </item>
</selector>
